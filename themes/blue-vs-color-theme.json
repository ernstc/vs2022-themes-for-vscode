	"tokenColors": [
		{
			"name": "Type alias",
			"scope": [
				"entity.name.type.alias.cs"
			],
			"settings": {
				"foreground": "#2B91AF"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#008000"
			}
		},
		{
			"name": "Keyword, delete, builtin function",
			"scope": [
				"keyword",
				"keyword.operator.instanceof",
				"variable.language",
				"support.function.builtin"
			],
			"settings": {
				"foreground": "#0000FF"
			}
		},
		{
			"name": "Storage, language constants",
			"scope": [
				"storage",
				"keyword.other",
				"keyword.operator.new",
				"keyword.operator.delete",
				"keyword.type.cs",
				"constant.language",
			],
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "String",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#a31515"
			}
		},
		{
			"name": "Escape sequence",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#b776fb"
			}
		},
		{
			"name": "Classes, Namespaces, T",
			"scope": [
				"entity.name.type",
			],
			"settings": {
				"foreground": "#2b91af"
			}
		},
		{
			"name": "operators",
			"scope": [
				"punctuation.definition.directive.cpp",
				"punctuation.definition.constant",
				"keyword.operator",
				"punctuation.definition.keyword",
				"storage.type.function.arrow",
				"punctuation.definition.variable"
			],
			"settings": {
				"foreground": "#000000" // unnecessary
			}
		},
		{
			"name": "Properties (aka: keys)",
			"scope": [
				"meta.property-name",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#2b91af"
			}
		},
		{
			"name": "Values",
			"scope": [
				"string.quoted.double.html",
				"string.quoted.double.xml"
			],
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "C++: String punctuation",
			"scope": [
				"punctuation.definition.string.begin.cpp",
				"punctuation.definition.string.end.cpp",
			],
			"settings": {
				"foreground": "#e21f1f"
			}
		},
		{
			"name": "C++: directives",
			"scope": [
				"keyword.control.directive",
			],
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"name": "C++: function, constructor",
			"scope": [
				"entity.name.function.definition.cpp",
				"entity.name.function.call.cpp",
				"entity.name.function.member.cpp",
				"entity.name.function.constructor.cpp",
			],
			"settings": {
				"foreground": "#74531f"
			}
		},
		{
			"name": "C++: parameter variable",
			"scope": [
				"variable.parameter.cpp",
			],
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"name": "C++: local variable",
			"scope": [
				"variable.other.local"
			],
			"settings": {
				"foreground": "#1f377f"
			}
		},
		{
			"name": "C++: preprocessor token, NULL, conditional token",
			"scope": [
				"meta.preprocessor.macro",
				"constant.language.NULL",
				"entity.name.other.preprocessor.macro.predefined"
			],
			"settings": {
				"foreground": "#8a1bff"
			}
		},
		{
			"name": "HTML: attribute",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#ff0000"
			}
		},
		{
			"name": "HTML: tag name",
			"scope": [
				"meta.tag.metadata",
				"meta.tag.structure"
			],
			"settings": {
				"foreground": "#800000"
			}
		},
		{
			"name": "HTML: defaults",
			"scope": [
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "XML: preprocessor, tag name",
			"scope": [
				"meta.tag.preprocessor.xml",
				"entity.name.tag.localname"
			],
			"settings": {
				"foreground": "#800000"
			}
		},
		{
			"name": "CSS: Selector",
			"scope": [
				"entity.name.tag.css"
			],
			"settings": {
				"foreground": "#800000"
			}
		},
		{
			"name": "CSS: id, class, pseudo class, Wildcard",
			"scope": [
				"entity.other.attribute-name.css",
				"entity.other.attribute-name.id.css",
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.pseudo-class.css",
				"entity.name.tag.wildcard"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#008800"
			}
		},
		{
			"name": "CSS: @media, functions",
			"scope": [
				"keyword.control.at-rule",
				"meta.function.shape.css",
				"meta.function.misc.css"
			],
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "CSS: defaults",
			"scope": [
				"keyword.other.unit",
				"constant.numeric.css"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "JAVA: non-primitive data types",
			"scope": [
				"storage.type.java",
				"punctuation.bracket.angle.java"
			],
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "JAVA: javadoc keyword",
			"scope": [
				"keyword.other.documentation.javadoc"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "JAVA: function definition",
			"scope": [
				"meta.method.identifier.java"
			],
			"settings": {
				"foreground": "#2b91af"
			}
		},
		{
			"name": "JAVA: defaults",
			"scope": [
				"variable.parameter.java",
				"punctuation.separator.delimiter.java"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "JS: defaults",
			"scope": [
				"entity.name.type.instance.jsdoc",
				"variable.other.jsdoc"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Python: function definition, support function",
			"scope": [
				"entity.name.function.python",
				"support.type.python"
			],
			"settings": {
				"foreground": "#2b91af"
			}
		},
		{
			"name": "Markdown: Heading, image-inline",
			"scope": [
				"markup.heading",
				"string.other.link.description.markdown",
				"meta.image.inline.markdown"
			],
			"settings": {
				"foreground": "#2b91af"
			}
		},
		{
			"name": "Markdown: seperator",
			"scope": [
				"meta.separator.markdown"
			],
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "Markdown: bold",
			"scope": [
				"markup.bold.markdown"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Bash: defaults",
			"scope": [
				"variable.other.normal.shell"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "C#: functions",
			"scope": [
				"entity.name.function.cs"
			],
			"settings": {
				"foreground": "#74531f"
			}
		},
		{
			"name": "C#: local & parameter variable",
			"scope": [
				"entity.name.variable.parameter.cs",
				"entity.name.variable.local.cs"
			],
			"settings": {
				"foreground": "#1f377f"
			}
		},
		{
			"name": "C#: Class attributes (variables)",		// temporary fix (wrong color for accessing class attributes)
			"scope": [
				"variable.other.readwrite.cs",
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "C#: Object variables (variables)",		// temporary fix (wrong color for static class names e.g "Console")
			"scope": [
				"variable.other.object.cs",
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "C#: defaults",
			"scope": [
				"entity.name.type.namespace.cs",
				"punctuation.parenthesis.open.cs",
				"punctuation.parenthesis.close.cs",
				"punctuation.terminator.statement.cs"
			],
			"settings": {
				"foreground": "#000000"
			}
		},		
	]
}
