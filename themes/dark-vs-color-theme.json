				"entity.name.tag.css"
			],
			"settings": {
				"foreground": "#5c9cd6"
			}
		},
		{
			"name": "CSS: Units",
			"scope": [
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#b5cea8"
			}
		},
		{
			"name": "CSS: id, class, pseudo class, Wildcard",
