	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#57a64a"
			}
		},
		{
			"name": "Keyword, delete, builtin function",
			"scope": [
				"keyword",
				"keyword.operator.instanceof",
				"variable.language",
				"support.function.builtin"
			],
			"settings": {
				"foreground": "#d8a0df"
			}
		},
		{
			"name": "Storage, language constants",
			"scope": [
				"storage",
				"keyword.other",
				"keyword.operator.new",
				"keyword.operator.delete",
				"keyword.type.cs",
				"constant.language",
			],
			"settings": {
				"foreground": "#5c9cd6"
			}
		},
		{
			"name": "String",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#d69d85"
			}
		},
		{
			"name": "Escape sequence",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#ffd68f"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#b5cea8"
			}
		},
		{
			"name": "Classes, Namespaces, T",
			"scope": [
				"entity.name.type"
			],
			"settings": {
				"foreground": "#4ec9b0"
			}
		},
		{
			"name": "Properties (aka: keys)",
			"scope": [
				"meta.property-name",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#4ec9b0"
			}
		},
		{
			"name": "Values",
			"scope": [
				"string.quoted.double.html",
				"string.quoted.double.xml"
			],
			"settings": {
				"foreground": "#c8c8c8"
			}
		},
		{
			"name": "C++: foreground",
			"scope": [
				"source.cpp",
			],
			"settings": {
				"foreground": "#c8c8c8"
			}
		},
		{
			"name": "C++: String punctuation",
			"scope": [
				"punctuation.definition.string.begin.cpp",
				"punctuation.definition.string.end.cpp",
			],
			"settings": {
				"foreground": "#e8c9bb"
			}
		},
		{
			"name": "C++: operators",
			"scope": [
				"punctuation.definition.constant",
				"keyword.operator",
				"punctuation.definition.keyword",
				"storage.type.function.arrow",
				"punctuation.definition.variable",
				"punctuation.terminator.statement.cpp",
				"punctuation.separator.scope-resolution.cpp",
				"punctuation.separator.delimiter.comma.cpp",
				"punctuation.section.block",
				"punctuation.section.parens",
				"punctuation.section.parameters"
			],
			"settings": {
				"foreground": "#b4b4b4"
			}
		},
		{
			"name": "C++: directives",
			"scope": [
				"keyword.control.directive",
			],
			"settings": {
				"foreground": "#9b9b9b"
			}
		},
		{
			"name": "C++: function, constructor",
			"scope": [
				"entity.name.function.definition.cpp",
				"entity.name.function.call.cpp",
				"entity.name.function.member.cpp",
				"entity.name.function.constructor.cpp"
			],
			"settings": {
				"foreground": "#dcdcaa"
			}
		},
		{
			"name": "C++: parameter variable",
			"scope": [
				"variable.parameter.cpp",
			],
			"settings": {
				"foreground": "#9a9a9a"
			}
		},
		{
			"name": "C++: local variable",
			"scope": [
				"variable.other.local"
			],
			"settings": {
				"foreground": "#9cdcfe"
			}
		},
		{
			"name": "C++: attribute",
			"scope": [
				"variable.other.property"
			],
			"settings": {
				"foreground": "#dadada"
			}
		},
		{
			"name": "C++: preprocessor token, NULL, conditional token",
			"scope": [
				"meta.preprocessor.macro",
				"constant.language.NULL",
				"entity.name.other.preprocessor.macro.predefined"
			],
			"settings": {
				"foreground": "#beb7ff"
			}
		},
		{
			"name": "C++: preprocessor conditional (#ifndef)",
			"scope": [
				"meta.preprocessor.conditional"
			],
			"settings": {
				"foreground": "#dcdcdc"
			}
		},
		{
			"name": "HTML: doctype",
			"scope": [
				"meta.tag.metadata.doctype"
			],
			"settings": {
				"foreground": "#5c9cd6"
			}
		},
		{
			"name": "HTML: attribute",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#92caf4"
			}
		},
		{
			"name": "HTML: tag name",
			"scope": [
				"meta.tag.metadata",
				"meta.tag.structure"
			],
			"settings": {
				"foreground": "#5c9cd6"
			}
		},
		{
			"name": "HTML: tag punctuation",
			"scope": [
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#b4b4b4"
			}
		},
		{
			"name": "XML: preprocessor, tag name",
			"scope": [
				"meta.tag.preprocessor.xml",
				"entity.name.tag.localname"
			],
			"settings": {
				"foreground": "#5c9cd6"
			}
		},
		{
			"name": "CSS: Selector",
